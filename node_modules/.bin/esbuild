#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nino/Documentos/Repositorios/api_rugby_events/node_modules/esbuild/bin/node_modules:/home/nino/Documentos/Repositorios/api_rugby_events/node_modules/esbuild/node_modules:/home/nino/Documentos/Repositorios/api_rugby_events/node_modules:/home/nino/Documentos/Repositorios/node_modules:/home/nino/Documentos/node_modules:/home/nino/node_modules:/home/node_modules:/node_modules:/home/nino/Documentos/Repositorios/api_rugby_events/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/nino/Documentos/Repositorios/api_rugby_events/node_modules/esbuild/bin/node_modules:/home/nino/Documentos/Repositorios/api_rugby_events/node_modules/esbuild/node_modules:/home/nino/Documentos/Repositorios/api_rugby_events/node_modules:/home/nino/Documentos/Repositorios/node_modules:/home/nino/Documentos/node_modules:/home/nino/node_modules:/home/node_modules:/node_modules:/home/nino/Documentos/Repositorios/api_rugby_events/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../esbuild/bin/esbuild"   "$@"
exit $?
